{
  "name": "kivisai-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "dev-clean": "npm run clean && npm run dev",
    "clean": "powershell -Command \"if (Test-Path .next) { Remove-Item -Recurse -Force .next }; if (Test-Path node_modules) { Remove-Item -Recurse -Force node_modules }; if (Test-Path package-lock.json) { Remove-Item package-lock.json }; npm install\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:vercel2": "powershell -ExecutionPolicy Bypass -File scripts/deploy-vercel2.ps1",
    "deploy:vercel2:unix": "bash scripts/deploy-vercel2.sh",
    "test:vercel2": "node scripts/test-vercel2-deployment.js",
    "analyze": "npm run build:analyze",
    "performance": "npm run build && npm run lighthouse",
    "lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "lighthouse:html": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "update-deps": "npm update",
    "update-deps:latest": "npx npm-check-updates -u && npm install",
    "pre-deploy": "npm run type-check && npm run lint && npm run test && npm run build",
    "post-deploy": "npm run performance",
    "monitor": "npm run dev & npm run lighthouse:html",
    "health-check": "node scripts/health-check.js",
    "performance-check": "node scripts/performance-check.js",
    "accessibility-check": "node scripts/accessibility-check.js",
    "seo-check": "node scripts/seo-check.js",
    "security-check": "node scripts/security-check.js",
    "full-audit": "npm run health-check && npm run performance-check && npm run accessibility-check && npm run seo-check && npm run security-check"
  },
